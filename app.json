{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./assets/Icon.png",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "name": "Smiles",
    "slug": "smiles",
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "6a1894c2-8174-40c7-aa8c-fde7842dbc8c"
      }
    },
    "owner": "lavawaffle",
    "android": {
      "package": "com.lavawaffle.smiles"
    },
    "ios": {
      "bundleIdentifier": "com.lavawaffle.smiles"
    }
  }
}
